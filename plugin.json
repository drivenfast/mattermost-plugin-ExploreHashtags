{
    "id": "com.ecf.hashtags",
    "name": "Hashtags",
    "description": "Channel hashtags browser and search",
    "homepage_url": "https://github.com/yourusername/mattermost-plugin-aggregatehashtags",
    "support_url": "https://github.com/yourusername/mattermost-plugin-aggregatehashtags/issues",
    "version": "1.0.0",
    "min_server_version": "6.2.1",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js",
        "loading": {
            "timeout": 30000
        }
    },
    "settings_schema": {
        "header": "Configure Hashtags Plugin",
        "footer": "",
        "settings": []
    },
  "permissions": [
    "read_channel",
    "read_public_channel",
    "read_private_channel"
  ],
  "app_bar": {
    "location": "library"
  }
}
